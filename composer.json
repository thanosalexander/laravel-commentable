{
    "name": "thanosalexander/laravel-commentable",
    "description": "Commentable Polymorphic Eloquent Models for for Laravel 5",
    "keywords": [
        "laravel",
        "framework",
        "Laravel-Commentable",
        "Laravel Commentable"
    ],
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "hello@basecode.sh",
            "homepage": "https://basecode.sh"
        },
        {
            "name": "Athanasios Alexandris",
            "email": "thanosalexander@gmail.com",
            "homepage": "https://thanosalexander.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^5.8|^6.0|^7.0|^8.0",
        "illuminate/database": "^5.8|^6.0|^7.0|^8.0",
        "kalnoy/nestedset": "^5.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0|^1.3.1",
        "phpunit/phpunit": "^8.0|^9.3"
    },
    "autoload": {
        "psr-4": {
            "Artisanry\\Commentable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artisanry\\Tests\\Commentable\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {

        "laravel": {
            "providers": [
                "Artisanry\\Commentable\\CommentableServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
